Task#5.1

p=int(input("Enter the value of a: "))
q=int(input("Enter the value of b: ")) 
def func(x,y):
    P=x+y
    Q=x-y
    R=x*y
    S=x//y
    print("Addition = ", P)
    print("Subtraction = ", Q)
    print("Multiplication = ", R)
    print("Division = ", S)
func(p,q)

